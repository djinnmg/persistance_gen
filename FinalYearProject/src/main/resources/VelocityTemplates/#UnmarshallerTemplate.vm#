package ${package}.jaxb.serialisation;

#foreach ($entity in $entities.entities)
	import ${package}.hibernate.entities.${entity.name};
	import ${package}.jaxb.types.${entity.name}Type;
#end
import org.joda.time.DateTime;
import java.util.Date;
	
public class Unmarshaller
{

	#foreach ($entity in $entities.entities)
		public $entity.name unmarshall(${entity.name}Type ${entity.getCamelCaseName}Type)
		{
			$entity.name $entity.name.toLowerCase = new ${entity.name}();
	
			#foreach ($property in $entity.properties) #* if date need to convert to DateTime *#
				${entity.getCamelCaseName}.${property.name} = ${entity.getCamelCaseName}Type.${property.name};			
			#end
			
			return ${entity.name};
		}
	#end

}
